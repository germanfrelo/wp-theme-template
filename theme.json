{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"name": "Base 1",
					"slug": "base-1",
					"color": "hsl(230deg 10% 100%)"
				},
				{
					"name": "Base 2",
					"slug": "base-2",
					"color": "hsl(230deg 10% 97%)"
				},
				{
					"name": "Contrast 1",
					"slug": "contrast-1",
					"color": "hsl(230deg 10% 10%)"
				},
				{
					"name": "Contrast 2",
					"slug": "contrast-2",
					"color": "hsl(230deg 10% 40%)"
				},
				{
					"name": "Accent",
					"slug": "accent-1",
					"color": "hsl(230deg 80% 55%)"
				},
				{
					"name": "Accent 2",
					"slug": "accent-2",
					"color": "hsl(230deg 80% 45%)"
				}
			]
		},
		"custom": {
			"borderRadius": {
				"small": "0.125rem"
			},
			"fontWeight": {
				"base": {
					"normal": "400",
					"medium": "500",
					"bold": "700"
				},
				"heading": "900"
			},
			"lineHeight": {
				"flat": "1",
				"small": "1.2",
				"base": "1.5"
			}
		},
		"layout": {
			"allowCustomContentAndWideSize": true,
			"contentSize": "50rem",
			"wideSize": "70rem"
		},
		"spacing": {
			"customSpacingSize": true,
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "3XS",
					"slug": "3-xs",
					"size": "var(--space-3xs)"
				},
				{
					"name": "3XS–2XS",
					"slug": "3-xs-2-xs",
					"size": "var(--space-3xs-2xs)"
				},
				{
					"name": "2XS",
					"slug": "2-xs",
					"size": "var(--space-2xs)"
				},
				{
					"name": "2XS–XS",
					"slug": "2-xs-xs",
					"size": "var(--space-2xs-xs)"
				},
				{
					"name": "XS",
					"slug": "xs",
					"size": "var(--space-xs)"
				},
				{
					"name": "XS–S",
					"slug": "xs-s",
					"size": "var(--space-xs-s)"
				},
				{
					"name": "S",
					"slug": "s",
					"size": "var(--space-s)"
				},
				{
					"name": "S–M",
					"slug": "s-m",
					"size": "var(--space-s-m)"
				},
				{
					"name": "M",
					"slug": "m",
					"size": "var(--space-m)"
				},
				{
					"name": "M–L",
					"slug": "m-l",
					"size": "var(--space-m-l)"
				},
				{
					"name": "L",
					"slug": "l",
					"size": "var(--space-l)"
				},
				{
					"name": "L–XL",
					"slug": "l-xl",
					"size": "var(--space-l-xl)"
				},
				{
					"name": "XL",
					"slug": "xl",
					"size": "var(--space-xl)"
				},
				{
					"name": "XL–2XL",
					"slug": "xl-2-xl",
					"size": "var(--space-xl-2xl)"
				},
				{
					"name": "2XL",
					"slug": "2-xl",
					"size": "var(--space-2xl)"
				},
				{
					"name": "2XL–3XL",
					"slug": "2-xl-3-xl",
					"size": "var(--space-2xl-3xl)"
				},
				{
					"name": "3XL",
					"slug": "3-xl",
					"size": "var(--space-3xl)"
				},
				{
					"name": "3XL–4XL",
					"slug": "3-xl-4-xl",
					"size": "var(--space-3xl-4xl)"
				},
				{
					"name": "4XL",
					"slug": "4-xl",
					"size": "var(--space-4xl)"
				}
			],
			"units": ["%", "ch", "em", "ex", "px", "rem", "vh", "vmax", "vmin", "vw"]
		},
		"typography": {
			"customFontSize": false,
			"defaultFontSizes": false,
			"fluid": false,
			"fontFamilies": [
				{
					"name": "Base",
					"slug": "base",
					"fontFamily": "'IBM Plex Sans', sans-serif",
					"fontFace": [
						{
							"fontFamily": "'IBM Plex Sans'",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/ibm-plex-sans/ibm-plex-sans-400-normal.woff2"
							]
						},
						{
							"fontFamily": "'IBM Plex Sans'",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/ibm-plex-sans/ibm-plex-sans-400-italic.woff2"
							]
						},
						{
							"fontFamily": "'IBM Plex Sans'",
							"fontWeight": "500",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/ibm-plex-sans/ibm-plex-sans-500-normal.woff2"
							]
						},
						{
							"fontFamily": "'IBM Plex Sans'",
							"fontWeight": "500",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/ibm-plex-sans/ibm-plex-sans-500-italic.woff2"
							]
						},
						{
							"fontFamily": "'IBM Plex Sans'",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/ibm-plex-sans/ibm-plex-sans-700-normal.woff2"
							]
						},
						{
							"fontFamily": "'IBM Plex Sans'",
							"fontWeight": "700",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/ibm-plex-sans/ibm-plex-sans-700-italic.woff2"
							]
						}
					]
				},
				{
					"name": "Display",
					"slug": "display",
					"fontFamily": "Merriweather, serif",
					"fontFace": [
						{
							"fontFamily": "Merriweather",
							"fontWeight": "900",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/merriweather/merriweather-900-normal.woff2"
							]
						},
						{
							"fontFamily": "Merriweather",
							"fontWeight": "900",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/merriweather/merriweather-900-italic.woff2"
							]
						}
					]
				},
				{
					"name": "Monospace",
					"slug": "mono",
					"fontFamily": "'JetBrains Mono', monospace",
					"fontFace": [
						{
							"fontFamily": "'JetBrains Mono'",
							"fontWeight": "400",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/jetbrains-mono/jetbrains-mono-400-normal.woff2"
							]
						},
						{
							"fontFamily": "'JetBrains Mono'",
							"fontWeight": "400",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/jetbrains-mono/jetbrains-mono-400-italic.woff2"
							]
						},
						{
							"fontFamily": "'JetBrains Mono'",
							"fontWeight": "700",
							"fontStyle": "normal",
							"src": [
								"file:./assets/fonts/jetbrains-mono/jetbrains-mono-700-normal.woff2"
							]
						},
						{
							"fontFamily": "'JetBrains Mono'",
							"fontWeight": "700",
							"fontStyle": "italic",
							"src": [
								"file:./assets/fonts/jetbrains-mono/jetbrains-mono-700-italic.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "XS",
					"slug": "xs",
					"size": "var(--step--2)"
				},
				{
					"name": "S",
					"slug": "s",
					"size": "var(--step--1)"
				},
				{
					"name": "M",
					"slug": "m",
					"size": "var(--step-0)"
				},
				{
					"name": "L",
					"slug": "l",
					"size": "var(--step-1)"
				},
				{
					"name": "XL",
					"slug": "xl",
					"size": "var(--step-2)"
				},
				{
					"name": "2XL",
					"slug": "2-xl",
					"size": "var(--step-3)"
				},
				{
					"name": "3XL",
					"slug": "3-xl",
					"size": "var(--step-4)"
				},
				{
					"name": "4XL",
					"slug": "4-xl",
					"size": "var(--step-5)"
				},
				{
					"name": "5XL",
					"slug": "5-xl",
					"size": "var(--step-6)"
				},
				{
					"name": "Inherit",
					"slug": "inherit",
					"size": "inherit"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"blockGap": "1.5em",
			"padding": {
				"left": "var:preset|spacing|m-l",
				"right": "var:preset|spacing|m-l"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|base",
			"fontSize": "var:preset|font-size|m",
			"fontWeight": "var:custom|font-weight|base|normal",
			"lineHeight": "var:custom|line-height|base"
		},
		"color": {
			"text": "var:preset|color|contrast",
			"background": "var:preset|color|base"
		},
		"css": "p { text-wrap: pretty; }",
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|display",
					"fontWeight": "var:custom|font-weight|heading"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var:preset|font-size|5-xl",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|4-xl",
					"lineHeight": "1.15"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var:preset|font-size|3-xl",
					"lineHeight": "1.2"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var:preset|font-size|2-xl",
					"lineHeight": "1.25"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var:preset|font-size|xl",
					"lineHeight": "1.3"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var:preset|font-size|l",
					"lineHeight": "1.35"
				}
			},
			"caption": {},
			"cite": {},
			"link": {},
			"button": {
				"spacing": {
					"padding": {
						"top": "0.75em",
						"right": "1.25em",
						"bottom": "0.75em",
						"left": "1.25em"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|s",
					"fontWeight": "var:custom|font-weight|base|medium",
					"lineHeight": "var:custom|line-height|small",
					"textDecoration": "none",
					"textTransform": "uppercase"
				},
				"color": {
					"text": "var:preset|color|base",
					"background": "var:preset|color|contrast"
				},
				"border": {
					"width": "2px",
					"style": "solid",
					"color": {
						"ref": "styles.elements.button.color.background"
					},
					"radius": "var:custom|border-radius|small"
				},
				":hover": {
					"border": {
						"color": {
							"ref": "styles.elements.button.:hover.color.background"
						}
					}
				},
				":focus": {
					"border": {
						"color": {
							"ref": "styles.elements.button.:focus.color.background"
						}
					}
				},
				":active": {
					"border": {
						"color": {
							"ref": "styles.elements.button.:active.color.background"
						}
					}
				}
			}
		},
